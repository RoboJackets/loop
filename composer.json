{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-redis": "*",
        "ext-uuid": "*",
        "ans-group/laravel-health-check": "2.1.1",
        "apereo/phpcas": "^1.6",
        "directorytree/ldaprecord-laravel": "*",
        "enlightn/security-checker": "^2.0",
        "guzzlehttp/guzzle": "^7.8",
        "htmlmin/htmlmin": "dev-master",
        "kiritokatklian/nova-permission": "5.1.2",
        "laravel/framework": "12.36.1",
        "laravel/horizon": "5.38.0",
        "laravel/nova": "5.7.6",
        "laravel/sanctum": "4.2.0",
        "laravel/scout": "10.20.0",
        "laravel/slack-notification-channel": "3.6.0",
        "laravel/tinker": "2.10.1",
        "meilisearch/meilisearch-php": "1.16.1",
        "quickbooks/v3-php-sdk": "^6.0",
        "sentry/sentry-laravel": "^4.13",
        "smalot/pdfparser": "^2.2",
        "spatie/laravel-csp": "2.10.3",
        "spatie/laravel-failed-job-monitor": "4.3.4",
        "spatie/laravel-permission": "6.21.0",
        "spatie/laravel-webhook-client": "3.4.4",
        "subfission/cas": "dev-master",
        "symfony/http-client": "^7.2",
        "symfony/postmark-mailer": "^7.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "3.5.5",
        "danog/advanced-json-rpc": "3.2.2",
        "fakerphp/faker": "1.24.1",
        "larastan/larastan": "3.4.0",
        "laravel/pail": "1.2.3",
        "laravel/pint": "1.25.1",
        "mockery/mockery": "1.6.12",
        "nunomaduro/collision": "8.8.2",
        "phan/phan": "5.5.2",
        "phpstan/extension-installer": "1.4.3",
        "phpstan/phpstan": "2.1.17",
        "phpstan/phpstan-deprecation-rules": "2.0.3",
        "phpstan/phpstan-strict-rules": "2.0.4",
        "psalm/plugin-laravel": "3.0.4",
        "slevomat/coding-standard": "8.24.0",
        "spatie/laravel-ignition": "2.9.1",
        "spatie/ray": "1.43.1",
        "squizlabs/php_codesniffer": "4.0.0",
        "vimeo/psalm": "6.13.1"
    },
    "replace": {
        "felixfbecker/advanced-json-rpc": "*",
        "paragonie/random_compat": "*",
        "rap2hpoutre/fast-excel": "*",
        "symfony/polyfill-apcu": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-messageformatter": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php54": "*",
        "symfony/polyfill-php55": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-util": "*",
        "symfony/polyfill-uuid": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ci": [
            "vendor/bin/pint --test",
            "vendor/bin/phpcs",
            "vendor/bin/phpstan analyse --level=max .",
            "vendor/bin/phan",
            "vendor/bin/psalm"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "classmap-authoritative": true,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "process-timeout": 90,
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        },
        "platform": {
            "php": "8.3.16"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com",
            "only": [
                "laravel/nova"
            ]
        },
        {
            "type": "vcs",
            "url": "https://github.com/RoboJackets/Laravel-HTMLMin.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/RoboJackets/cas.git"
        }
    ]
}
