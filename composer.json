{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "adldap2/adldap2-laravel": "^6.1",
        "ans-group/laravel-health-check": "^1.13",
        "enlightn/enlightn": "^2.1",
        "guzzlehttp/guzzle": "^7.5",
        "htmlmin/htmlmin": "^9.0",
        "jeffbeltran/sanctum-tokens": "^2.0",
        "laravel/framework": "^9.19",
        "laravel/horizon": "^5.10",
        "laravel/nova": "^4.13",
        "laravel/sanctum": "^3.0",
        "laravel/scout": "^9.4",
        "laravel/slack-notification-channel": "^2.4",
        "laravel/tinker": "^2.7",
        "predis/predis": "^2.0",
        "robojackets/meilisearch-index-settings-helper": "^0.8.0",
        "sentry/sentry-laravel": "^2.13",
        "smalot/pdfparser": "^2.2",
        "spatie/laravel-csp": "^2.8",
        "spatie/laravel-failed-job-monitor": "^4.1",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-webhook-client": "^3.1",
        "subfission/cas": "^4.2",
        "symfony/http-client": "^6.1",
        "symfony/postmark-mailer": "^6.1",
        "vyuldashev/nova-permission": "^3.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.2",
        "phan/phan": "^5.4",
        "phpstan/extension-installer": "^1.1",
        "phpunit/phpunit": "^9.5.10",
        "psalm/plugin-laravel": "^2.0",
        "slevomat/coding-standard": "^8.4",
        "spatie/laravel-ignition": "^1.0",
        "spatie/ray": "^1.36"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "rap2hpoutre/fast-excel": "*",
        "symfony/polyfill-apcu": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-messageformatter": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php54": "*",
        "symfony/polyfill-php55": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-util": "*",
        "symfony/polyfill-uuid": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ci": [
            "vendor/bin/pint --test",
            "vendor/bin/phpcs",
            "vendor/bin/phpstan analyse --level=max .",
            "vendor/bin/phan",
            "vendor/bin/psalm"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "classmap-authoritative": true,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "process-timeout": 90,
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com",
            "only": [
                "laravel/nova"
            ]
        },
        {
            "type": "composer",
            "url": "https://composer.robojackets.org",
            "only": [
                "robojackets/*",
                "oitnetworkservices/*"
            ]
        }
    ]
}
